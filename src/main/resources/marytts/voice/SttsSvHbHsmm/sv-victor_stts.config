#Auto-generated config file for voice victor_stts

name = victor_stts
sv-voice.version = trunk

voice.version = trunk

# Declare "group names" as component that other components can require.
# These correspond to abstract "groups" of which this component is an instance.
provides = \
         sv-voice \
         hmm-voice

# List the dependencies, as a whitespace-separated list.
# For each required component, an optional minimum version and an optional
# download url can be given.
# We can require a component by name or by an abstract "group name"
# as listed under the "provides" element.
requires = \
   sv \
   marybase 

requires.marybase.version = trunk
requires.sv.version = trunk
requires.sv.download = http://mary.dfki.de/download/mary-install-4.x.x.jar
requires.hmm.version = trunk

####################################################################
####################### Module settings  ###########################
####################################################################
# For keys ending in ".list", values will be appended across config files,
# so that .list keys can occur in several config files.
# For all other keys, values will be copied to the global config, so
# keys should be unique across config files.

hmm.voices.list = \
   victor_stts

# If this setting is not present, a default value of 0 is assumed.
voice.victor_stts.wants.to.be.default = 0

# Set your voice specifications
voice.victor_stts.gender = male
voice.victor_stts.locale = sv
voice.victor_stts.domain = general
voice.victor_stts.samplingRate = 16000

# HMM Voice-specific parameters 
# parameters used during models training 
# MGC: stage=gamma=0 alpha=0.42 linear gain (default) 
# LSP: gamma>0  
#          LSP: gamma=1 alpha=0.0  linear gain/log gain 
#      Mel-LSP: gamma=1 alpha=0.42 log gain 
#      MGC-LSP: gamma=3 alpha=0.42 log gain 
voice.victor_stts.alpha = 0.42
voice.victor_stts.gamma = 0
voice.victor_stts.logGain = false

# Parameter beta for postfiltering 
voice.victor_stts.beta = 0.0

# HMM Voice-specific files
# Information about trees
voice.victor_stts.Ftd = MARY_BASE/lib/voices/victor_stts/tree-dur.inf
voice.victor_stts.Ftf = MARY_BASE/lib/voices/victor_stts/tree-lf0.inf
voice.victor_stts.Ftm = MARY_BASE/lib/voices/victor_stts/tree-mgc.inf
voice.victor_stts.Fts = MARY_BASE/lib/voices/victor_stts/tree-str.inf

# Information about means and variances PDFs 
voice.victor_stts.Fmd = MARY_BASE/lib/voices/victor_stts/dur.pdf
voice.victor_stts.Fmf = MARY_BASE/lib/voices/victor_stts/lf0.pdf
voice.victor_stts.Fmm = MARY_BASE/lib/voices/victor_stts/mgc.pdf
voice.victor_stts.Fms = MARY_BASE/lib/voices/victor_stts/str.pdf

# Information about Global Mean and Variance PDFs
voice.victor_stts.useGV = true
voice.victor_stts.maxMgcGvIter = 200
voice.victor_stts.maxLf0GvIter = 200
voice.victor_stts.Fgvf = MARY_BASE/lib/voices/victor_stts/gv-lf0-littend.pdf
voice.victor_stts.Fgvm = MARY_BASE/lib/voices/victor_stts/gv-mgc-littend.pdf
voice.victor_stts.Fgvs = MARY_BASE/lib/voices/victor_stts/gv-str-littend.pdf

# A context features file example for start-up testing.
voice.victor_stts.FeaFile = MARY_BASE/lib/voices/victor_stts/8sidor_sentences3535.pfeats

# Tricky phones file in case there were problematic phones during training, empty otherwise.
voice.victor_stts.trickyPhonesFile = MARY_BASE/lib/voices/victor_stts/trickyPhones.txt

# Information about Mixed Excitation
voice.victor_stts.useMixExc = true

# Filters for mixed excitation 
# File format: one column with all the taps, where the number of taps per filter = numTaps/numFilters 
voice.victor_stts.Fif = MARY_BASE/lib/voices/victor_stts/mix_excitation_filters.txt
# Number of filters in bandpass bank
voice.victor_stts.in = 5

# Information about acousticModels (if true allows prosody modification specified in MARYXML input)
voice.victor_stts.useAcousticModels = true

# acoustic models to use (HMM models or carts from other voices can be specified)
#(uncoment to allow prosody modification specified in MARYXML input)
voice.victor_stts.acousticModels = duration F0

voice.victor_stts.duration.model = hmm
voice.victor_stts.duration.data = MARY_BASE/conf/sv-victor_stts.config
voice.victor_stts.duration.attribute = d

voice.victor_stts.F0.model = hmm
voice.victor_stts.F0.data = MARY_BASE/conf/sv-victor_stts.config
voice.victor_stts.F0.attribute = f0

